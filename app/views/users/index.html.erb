<% provide(:title, 'All Users') %>
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <div class="title-with-button"><h1>All Users</h1></div>
    </div>
  </div>
  <div class="row">
    <div class="col-lg-12"><%= will_paginate %></div>
  </div>
  <div class="row">
    <div class="col-lg-12">
      <ul class="users">
        <%= render @users %>
      </ul>
    </div>
  </div>
  <div class="row">
    <div class="col-lg-12"><%= will_paginate %></div>
  </div>
</div>
